{
  "name": "tailv",
  "author": "jaskang",
  "version": "0.1.11",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.cjs",
      "import": "./dist/preset.js"
    }
  },
  "scripts": {
    "dev": "npm-run-all --parallel docs:dev",
    "docs:dev": "vitepress dev site",
    "docs:build": "pnpm build && vitepress build site",
    "docs:preview": "vitepress preview site",
    "preset:dev": "tsup --watch",
    "build": "vite build && tsup",
    "change": "pnpm changeset",
    "release": "pnpm build && changeset publish"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.1",
    "@vueuse/core": "^10.11.0",
    "kotl": "^2.1.4",
    "mitt": "^3.0.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@ctrl/tinycolor": "^4.1.0",
    "@heroicons/vue": "^2.1.5",
    "@tailwindcss/vite": "^4.0.0-alpha.17",
    "@jaskang/eslint-config": "^3.1.0",
    "@jaskang/tsconfig": "^1.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.14.10",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.6.0",
    "happy-dom": "^14.12.3",
    "lucide-vue-next": "^0.407.0",
    "mdast-util-from-markdown": "^2.0.1",
    "mdast-util-frontmatter": "^2.0.1",
    "mdast-util-to-markdown": "^2.1.0",
    "micromark-extension-frontmatter": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.0.0-alpha.17",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "unist-util-visit": "^5.0.0",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-inspect": "^0.8.4",
    "vite-plugin-vue-devtools": "^7.3.5",
    "vitepress": "1.2.2",
    "vitest": "^2.0.2",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      },
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  },
  "engines": {
    "pnpm": ">=8.0.0"
  }
}
