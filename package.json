{
  "name": "root",
  "private": true,
  "author": "jaskang",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build": "vite build",
    "vue-tsc": "vue-tsc --declaration --emitDeclarationOnly ",
    "build-types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json --composite false && api-extractor run -c api-extractor.json && rimraf dist/src",
    "preview": "vite preview",
    "test": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.2",
    "@heroicons/vue": "^2.0.18",
    "@vueuse/core": "^10.4.1",
    "kotl": "^1.7.0"
  },
  "devDependencies": {
    "@jaskang/config": "^2.0.0",
    "@jaskang/eslint-config": "^2.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jsdom": "^21.1.2",
    "@types/node": "^20.6.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "happy-dom": "^11.0.2",
    "mini-svg-data-uri": "^1.4.4",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "rimraf": "^5.0.1",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-inspect": "^0.7.38",
    "vite-plugin-markdown-preview": "^1.1.1",
    "vitepress": "1.0.0-rc.12",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.10"
  },
  "engines": {
    "node": ">=16.18.0",
    "pnpm": ">=8.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
