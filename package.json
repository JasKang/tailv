{
  "name": "elenext",
  "version": "1.0.0-beta.0",
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "dev": "vite",
    "build": "vite build",
    "vue-tsc": "vue-tsc --declaration --emitDeclarationOnly",
    "build-types": "vue-tsc --declaration --emitDeclarationOnly tsconfig.build.json && api-extractor run -c api-extractor.json && rimraf dist/src",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@jaskang/config": "^0.0.7",
    "@jaskang/eslint-config": "^0.0.7",
    "@microsoft/api-extractor": "^7.33.7",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^16.18.11",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/test-utils": "^2.2.7",
    "@vue/theme": "^1.3.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitepress": "1.0.0-alpha.35",
    "vitest": "^0.26.3",
    "vue-tsc": "^1.0.22"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
