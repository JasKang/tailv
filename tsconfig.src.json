{
  "extends": "@jaskang/config/tsconfig.vue.json",
  "compilerOptions": {
    "composite": true,
    "types": ["vite/client"],
    "paths": {
      "tailv": ["./src"],
      "@/*": ["./src/*"]
    }
  },
  "include": ["./site/.vitepress/**/*", "./src/**/*"],
  "exclude": ["./site/.vitepress/cache", "./site/.vitepress/config.ts"]
}
